n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const {isLoading, isAuth} = useSelector(state => state.login)\n\n  const dispatch = useDispatch()\n \n\n  const handleOnChange = (e) => {\n    const { name, value } = e.target;\n\n    switch (name) {\n      case \"email\":\n        setEmail(value);\n        break;\n      case \"password\":\n        setPassword(value);\n        break;\n      default:\n        break;\n    }\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    if (!email || !password) {\n      return alert(\"Please fill in the form\");\n    }\n\n    dispatch(loginPending())\n\n    try {\n      const isAuth = await userLogin({email, password})\n      console.log(isAuth)\n\n\n    } catch (error) {\n      dispatch(loginFail(error.message))\n\n    }\n  };\n  return (\n    <div className={classes.div}>\n      <Paper className={classes.paper}>\n        <Typography className={classes.title}>Client Login</Typography>\n\n        <form className={classes.form}>\n          <TextField\n            InputProps={{\n              disableUnderline: true,\n            }}\n            variant=\"filled\"\n            margin=\"normal\"\n            required\n            id=\"email\"\n            label=\"Enter Email\"\n            name=\"email\"\n            autoComplete=\"email\"\n            className={classes.input}\n            onChange={handleOnChange}\n            value={email}\n          />\n\n          <TextField\n            InputProps={{\n              disableUnderline: true,\n            }}\n            variant=\"filled\"\n            margin=\"normal\"\n            required\n            id=\"password\"\n            label=\"Password\"\n            name=\"password\"\n            autoComplete=\"password\"\n            className={classes.input}\n            onChange={handleOnChange}\n            value={password}\n          />\n        </form>\n\n        <Button onClick={handleSubmit} className={classes.button}>\n          Login\n        </Button>\n        {isLoading && <CircularProgress className = {classes.spinner} />}\n      </Paper>\n    </div>\n  );\n}\n","/Users/thepaynefamily/Desktop/vs-code/react/ticket-system-main/ticket-system/src/Components/Layout/Header.jsx",[],"/Users/thepaynefamily/Desktop/vs-code/react/ticket-system-main/ticket-system/src/Components/Login/LoginSlice.jsx",[],"/Users/thepaynefamily/Desktop/vs-code/react/ticket-system-main/ticket-system/src/Components/TicketSlice/TicketSlice.jsx",[],"/Users/thepaynefamily/Desktop/vs-code/react/ticket-system-main/ticket-system/src/store.js",[],"/Users/thepaynefamily/Desktop/vs-code/react/ticket-system-main/ticket-system/src/api/userAPI.jsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":9,"column":24,"nodeType":"80","messageId":"81","endLine":9,"endColumn":36},{"ruleId":"78","severity":1,"message":"82","line":81,"column":10,"nodeType":"80","messageId":"81","endLine":81,"endColumn":18},{"ruleId":"78","severity":1,"message":"83","line":81,"column":20,"nodeType":"80","messageId":"81","endLine":81,"endColumn":31},{"ruleId":"78","severity":1,"message":"84","line":84,"column":21,"nodeType":"80","messageId":"81","endLine":84,"endColumn":27},"no-native-reassign",["85"],"no-negated-in-lhs",["86"],"no-unused-vars","'loginSuccess' is defined but never used.","Identifier","unusedVar","'formLoad' is assigned a value but never used.","'setFormLoad' is assigned a value but never used.","'isAuth' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]